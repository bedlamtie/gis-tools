{
  "name": "gis-tools",
  "type": "module",
  "version": "v1.0.0",
  "description": "> 用于Node.js的GIS工具处理库，主要用于处理 GIS 数据。",
  "author": "Jiam",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.esm.js",
      "require": "./index.cjs.js",
      "default": "./index.esm.js"
    }
  },
  "main": "./index.cjs.js",
  "module": "./index.esm.js",
  "types": "./index.d.ts",
  "scripts": {
    "test:split": "esno examples/split_geojson_pbf/index.ts",
    "build": "esno scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "pack": "esno scripts/pack.ts",
    "clean": "rimraf dist",
    "watch": "esno scripts/build.ts --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspector": "pnpm dlx @eslint/config-inspector"
  },
  "dependencies": {
    "@mapbox/togeojson": "^0.16.2",
    "@turf/turf": "^7.0.0",
    "chalk": "^4.1.2",
    "consola": "^3.2.3",
    "fs-extra": "^11.2.0",
    "gcoord": "^1.0.6",
    "geobuf": "^3.0.2",
    "image-size": "^1.1.1",
    "pbf": "^4.0.1",
    "proj4": "^2.11.0",
    "shapefile": "^0.6.6",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.23.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@types/cli-progress": "^3.11.6",
    "@types/fs-extra": "^11.0.4",
    "@types/geobuf": "^3.0.4",
    "@types/node": "^20.14.10",
    "@types/proj4": "^2.5.5",
    "@types/shapefile": "^0.6.4",
    "@types/xmldom": "^0.1.34",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "esbuild-register": "^3.5.0",
    "eslint": "^9.7.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "lint-staged": "^15.2.8",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
